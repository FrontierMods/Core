  {
    "id": "medical_gauze",
    "copy-from": "medical_gauze",
    "type": "COMESTIBLE",
    "comestible_type": "MED",

    "//sources": [
      "https://www.buyemp.com/product/curaplex-gauze-pads/group001457"
    ],

    "//notes": [
      "4-inch × 4-inch sterile swabs",
      "while the description says this swab is sealed, we ignore this and presume that, like so many medical comestibles in the game, this one is transported without cover, without losing its sterility"
    ],

    "name": {
      "str": "medical gauze"
    },

    "//weight": "basic weight (density; 13.2 g/m3) × area of swab (4 in ^ 2 = 16 in2) × ply (layers; 12)",
    "weight": "1635 mg",
    "//dimensions": {
      "length": "4 in",
      "width": "4 in",
      "height": "0.206 mm",
      "raw": "2 ml",
      "trim": 0,
      "notes": [
        "height is calculated as follows",
        {
          "basic_weight": "13.2 g/m2",
          "ply": 12,
          "density_cotton": "1.54 g/cm3",
          "density_factor": "0.5",

          "weight_per_m2": "basic_weight * ply",
          "weight_per_cm2": "weight_per_m2 / 10000",
          "density_per_ply": "density_cotton * density_factor",

          "thickness": "weight_per_cm2 / density_per_ply",

          "notes": [
            "weight_per_m2 is converted to weight_per_cm2 (g/cm2) in order to calculate over density (g/cm3)"
          ]
        }
      ]
    },
    "volume": "2 ml",
    "longest_side": "102 mm"
  }
